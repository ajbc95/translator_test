{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Hangfire": "Information",
      "Hangfire.SqlServer": "Information",
      "Hangfire.Processing": "Information"
    }
  },
  "AzureTranslator": {
    "Endpoint": "https://api.cognitive.microsofttranslator.com/",
    "ApiKey": "<apiKey>",
    "Region": "<region>"
  },
  "ConnectionStrings": {
    "Translator": "<translatorConnString>"
  },
  "AllowedHosts": "*"
}